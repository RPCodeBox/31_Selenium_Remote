{
  "capabilities": [
    {
      "browserName": "firefox",
      "browser-version": "68",
      "maxInstances": 5,
      "platform": "Windows 10",
	  "seleniumProtocol": "WebDriver",
	  "webdriver.gecko.driver": "geckodriver.exe",
      "acceptSslCerts": true,
      "javascriptEnabled": true,
      "takesScreenshot": false,
      "cleanSession": true,
      "FirefoxDriver.SystemProperty.BROWSER_LOGFILE": "firefox.log"
    },
    {
      "browserName": "chrome",
      "maxInstances": 5,
      "platform": "Windows 10",
	  "seleniumProtocol": "WebDriver",
      "webdriver.chrome.driver": "chromedriver.exe"
    },
    {
      "browserName": "internet explorer",
      "maxInstances": 3,
      "platform": "Windows 10",
	  "seleniumProtocol": "WebDriver",
      "webdriver.ie.driver": "IEDriverServer.exe"
    }
  ],
    "cleanUpCycle": 2000,
    "timeout": 30000,
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "port": 5555,
    "register": true,
	"registerCycle": 5000,
    "maxSession": 5,
	"nodeStatusCheckTimeout": 5000,
	"nodePolling": 5000,
	"unregisterIfStillDownAfter": 60000,
	"downPollingLimit": 2,
	"debug": false,
	"servlets" : [],
	"withoutServlets": [],
	"custom": {}
}